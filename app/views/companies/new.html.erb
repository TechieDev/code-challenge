<%= form_for @company do |f| %>

	 <%= f.label :name, "Company Name" %><br>
     <%= f.text_field :name %>

	 <%= f.label :city %><br>
     <%= f.text_field :city %>

	 <%= f.label :state %><br>
     <%= f.text_field :state %>

	 <%= f.label :founded_date %><br>
     <%= f.text_field :founded_date %>

	 <%= f.label :description %><br>
     <%= f.text_area :description %>

     <%= f.submit 'Create Company' %>
<% end %>